{
  "name": "cicd-pipeline-dashboard",
  "version": "1.0.0",
  "description": "A comprehensive CI/CD Pipeline Health Dashboard for monitoring GitHub Actions, Jenkins, and other CI/CD tools",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "node src/database/migrate.js",
    "db:seed": "node src/database/seed.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "ci-cd",
    "pipeline",
    "dashboard",
    "monitoring",
    "github-actions",
    "jenkins",
    "real-time",
    "alerts"
